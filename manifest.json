{
  "manifest_version": 3,
  "name": "McPlanning",
  "version": "1.0",
  "description": "Convert Mcdo email planning into ICal file.",
  "icons": {
    "16": "./images/icon-16x16.png",
    "32": "./images/icon-32x32.png",
    "48": "./images/icon-48x48.png",
    "128": "./images/icon-128x128.png"
  },
  "action": {
    "default_popup": "./popup.html",
    "default_icons": {
        "16": "./images/icon-16x16.png",
        "32": "./images/icon-32x32.png",
        "48": "./images/icon-48x48.png",
        "128": "./images/icon-128x128.png"
    }
  },
  "permissions": [
    "activeTab",
    "scripting"
  ]
}